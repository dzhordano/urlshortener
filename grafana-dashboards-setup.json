{
  "dashboard": {
    "id": null,
    "title": "some something with some something",
    "tags": ["monitoring", "logs", "metrics", "traces"],
    "timezone": "browser",
    "schemaVersion": 36,
    "version": 0,
    "refresh": "30s"
  },
  "panels": [
    {
      "id": 1,
      "title": "CPU Usage",
      "type": "timeseries",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total[5m]) * 100"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "Memory Usage",
      "type": "timeseries",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "go_memstats_alloc_bytes"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
    },
    {
      "id": 3,
      "title": "Logs Explorer",
      "type": "logs",
      "datasource": "Loki",
      "targets": [
        {
          "expr": "{job=\"app\"}",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 12, "w": 24, "x": 0, "y": 8}
    },
    {
      "id": 4,
      "title": "Traces Explorer",
      "type": "traces",
      "datasource": "Jaeger",
      "targets": [],
      "gridPos": {"h": 12, "w": 24, "x": 0, "y": 20}
    }
  ]
}